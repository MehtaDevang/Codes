import java.util.*;
import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.lang.*;
public class EasyOne {
	public static void main(String[] args) throws Exception{
		BufferedReader scanner = new BufferedReader(new InputStreamReader(System.in));
		
		int t = Integer.parseInt(scanner.readLine());
		for(int i = 0; i < t; i++) {
			String[] str = scanner.readLine().trim().split(" ");
			int n = Integer.parseInt(str[0]);
			int q = Integer.parseInt(str[1]);
			HashMap<Long, Integer> map = new HashMap<>();
			String[] temp = scanner.readLine().trim().split(" ");
			for(int j = 0; j < n; j++) {
				if(!map.containsKey(Integer.parseInt(temp[j]))) {
					map.put(Long.parseLong(temp[j]), 1);
				}
				else {
					map.put(Long.parseLong(temp[j]), map.get(Integer.parseInt(temp[j])) + 1);
				}
			}
			for(Map.Entry<Long, Integer> entry : map.entrySet()){
				System.out.println(entry.getKey() + "---->" + entry.getValue());
			}
			for(int j = 0; j < q; j++) {
				long test = Long.parseLong(scanner.readLine());
				int val = 0;
				if(map.containsKey(test)) {
					val = map.get(test);
				}
				System.out.println(val);
			}
		}
	}
}
