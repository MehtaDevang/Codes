import java.util.*;
public class PrimeProblems {
	static long sum;
	public static boolean isPrime(int n) {
		boolean flag = true;
		if(n == 2 || n == 3) {
			return true;
		}
		else {
			for(int i = 2; i < n; i++) {
				if(n % i == 0) {
					flag = false;
					break;
				}
				else {
					continue;
				}
			}
			return flag;
		}		
	}
	public static void count(int[] arr, int index) {
		int len = arr.length;
//		while(index < len) {
//			if(index+1 <= len) {
//				if(index+1 == 1) {
//					sum = sum + arr[index];
//				}
//				else if(isPrime(index+1)) {
//					sum = sum + arr[index];
//				}
//			}
//			index++;
//		}
		
		int c = 2;
		while(len > 0) {
			if(isPrime(len)) {
				c = len;
				break;
			}
			len--;
		}
		for(int i = 0; i < c; i++) {
			sum = sum + arr[i];
		}
	}
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int n = Integer.parseInt(scanner.nextLine());
		String[] temp = scanner.nextLine().trim().split(" ");
		int[] arr = new int[n];
		for(int i = 0; i < n; i++) {
			arr[i] = Integer.parseInt(temp[i]);
		}
		sum = 0;
		count(arr, 0);
		System.out.println(sum);
	}
}
