import java.util.*;
import java.io.*;

public class StackMin{
	int minElement = 0;
	
	public void stack_push(java.util.Stack<Integer> stack) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("Element:");
		int num = scanner.nextInt();
		if(stack.isEmpty()) {
			minElement = num;
			stack.push(num);
		}
		else {
			if(num >= minElement) {
				stack.push(num);
			}
			else {
				minElement = num;
				stack.push((2*num)-minElement);
			}
		}
		System.out.println("pushed element is "+ num + " and min is " + minElement);
	}
	
	public void stack_pop(java.util.Stack<Integer> stack) {
		int num = stack.pop();
		System.out.println(num);
		if(num < minElement) {
			minElement = (2 * minElement) - num;
		}
//		System.out.println("popped element is "+ num + " and min is " + minElement);
	}
	
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		StackMin obj = new StackMin();
		java.util.Stack<Integer> stack = new java.util.Stack<Integer>();
		
		int i = 0;
		while(i < 6) {
			obj.stack_push(stack);
			i++;
		}
		for(int j = 0; j < 3; j++) {
			obj.stack_pop(stack);
		}
	}
}
